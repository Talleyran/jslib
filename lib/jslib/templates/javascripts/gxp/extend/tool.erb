<%#@name%>Ext.namespace "gxp.plugins"
gxp.plugins.<%= @name.camelize %> = Ext.extend(gxp.plugins.Tool,
  ptype: "gxp_<%= @name.delete "_" %>"
  <%= @name.camelize(:lower) %>Tooltip: '<%= @name.humanize %>'
  <%= @name.camelize(:lower) %>Text: '<%= @name.humanize %>'
  constructor: (config) ->
    gxp.plugins.<%= @name.camelize %>.superclass.constructor.apply this, arguments

  init: (target)->
    gxp.plugins.<%= @name.camelize %>.superclass.init.apply this, arguments

  addActions: ->
    @button = new Ext.Button(
      text: @<%= @name.camelize(:lower) %>Text
      tooltip: @<%= @name.camelize(:lower) %>Tooltip
    )
    gxp.plugins.<%= @name.camelize %>.superclass.addActions.apply this, [ @button ]

  addOutput: ->
    gxp.plugins.<%= @name.camelize %>.superclass.constructor.addOutput this, arguments
)
Ext.preg gxp.plugins.<%= @name.camelize %>::ptype, gxp.plugins.<%= @name.camelize %>
